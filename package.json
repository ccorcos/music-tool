{
	"name": "typescript-boilerplate",
	"version": "1.0.1",
	"description": "boilerplate for web applications using typescript and webpack",
	"main": "index.js",
	"scripts": {
		"watch": "ts-node -T src/tools/build.ts --watch",
		"server": "ts-node -T src/tools/server.ts",
		"start": "run-p watch server",
		"clean": "rm -rf build",
		"build": "ts-node -T src/tools/build.ts",
		"deploy": "ts-node -T src/tools/deploy.ts",
		"release": "run-s clean build deploy"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@types/chroma-js": "^2.1.0",
		"chroma-js": "^2.1.0",
		"glamor": "^2.20.40",
		"lodash": "^4.17.20",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"tone": "^14.7.77"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/livereload": "^0.9.1",
		"@types/node": "^16.9.1",
		"connect-livereload": "^0.6.1",
		"cpx": "^1.5.0",
		"estrella": "^1.4.1",
		"express": "^4.17.1",
		"livereload": "^0.9.3",
		"npm-run-all": "^4.1.5",
		"@types/lodash": "^4.14.164",
		"@types/react": "^16.9.55",
		"@types/react-dom": "^16.9.9",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.5"
	}
}
